{% block steered_customer_registration_overview %}
    <sw-page class="steered-customer-registration-list">
        {% block swag_bundle_list_smart_bar_actions %}
            <template slot="smart-bar-actions">
                <sw-button variant="submit" >
                    {{ $t('as-steered-customer-registration.general.uploadWhitelist') }}
                </sw-button>
            </template>
        {% endblock %}

        <template slot="content">
            {% block steered_customer_registration_list_content %}
                <sw-entity-listing
                    v-if="invitations"
                    :items="invitations"
                    :repository="repository"
                    :showSelection="false"
                    :columns="columns">
                </sw-entity-listing>
            {% endblock %}
        </template>
    </sw-page>
{% endblock %}